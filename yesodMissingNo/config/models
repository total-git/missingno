Area
    area_description Text
    name Text Maybe
    go_north Text Maybe
    go_east Text Maybe
    go_south Text Maybe
    go_west Text Maybe
    deriving Show
Item
    item_description Text
    name Text
    use_action Text
    exact_location Text Maybe
    takeable Bool
    area_id AreaId
    deriving Show
Player_status
    area_id AreaId
    time UTCTime Maybe
    deriving Show
Item_status
    player_id Player_statusId
    item_id ItemId
    status Text
    deriving Show

